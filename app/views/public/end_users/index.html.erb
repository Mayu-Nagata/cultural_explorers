<div class="container">
  <div class="row">
    <%= render'profile', end_user:@end_user %>
    <div class="mt-3 col-md-6 offset-md-1">
    <div class="mt-3 mx-auto">
      <h2>Users</h2>
      <table class="table table-hover table-inverse">
        <thread>

        </thread>
        <tbody>
          <% @end_users.each do |end_user| %>
          <tr>
            <td><%=link_to end_user_path(end_user.id) do %><%= image_tag end_user.profile_image, style: "border-radius: 50%; width: 100px; height: 100px; object-fit:cover;" %><% end %></td>
            <td><%=link_to end_user_path(end_user.id) do %><%= end_user.name %><% end %></td>
            <td><%= end_user.introduction %></td>
          <% end %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>