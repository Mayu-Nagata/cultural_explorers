<div class="container">
  <div class="row">
    <div class="mt-3 mx-auto">
      <h2><b>会員一覧</b></h2>
      <table class="table">
        <thead>
          <tr>
            <tbody>
              <% @end_users.each do |end_user| %>
                <td><%= end_user.id %></td>
                <td><u><%= link_to admin_end_user_path(end_user.id) do %><%= end_user.name%><% end %></u></td>
                <td><%= end_user.email %></td>
                <% if end_user.is_deleted %>
                <td class="text-secondary"><strong>無効</strong></td>
              <% else %>
                <td class="text-success"><strong>有効</strong></td>
              <% end %>
          </tr>
              <% end %>

            </tbody>
          </tr>
        </thead>
      </table>
    </div>
  </div>
</div>
